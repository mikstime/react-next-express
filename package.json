    "react-dom": "^17.0.2",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "styled-components": "^5.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
